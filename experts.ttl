                                                        <http://emmo.info/emmo/domain/characterisation-methods> ,
                                                        <http://emmo.info/emmo/domain/manufacturing> ,
                                                        <http://emmo.info/emmo/domain/materials> ,
                                                        <http://emmo.info/emmo/domain/models> ,
                                                        <http://emmo.info/emmo/domain/software> ;
